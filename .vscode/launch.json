  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Local",
      "type": "node",
      "request": "attach",
      "port": 5865,
      "address": "127.0.0.1",
      "restart": false,
      "sourceMaps": false,
      "outFiles": [],
      "remoteRoot": "${workspaceRoot}",
      "localRoot": "${workspaceRoot}",
      "protocol": "inspector",
      "skipFiles": [
        "!**/node_modules/**",
        "**/$KNOWN_TOOLS$/**",
        "<node_internals>/**",
        "<node_internals>/internal/async_hooks.js",
        "<node_internals>/internal/inspector_async_hook.js"
      ]
    }
  ]
}

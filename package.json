  "main": "app.js",
  "scripts": {
    "start": "NODE_OPTIONS='--experimental-global-webcrypto' nodemon --inspect=0.0.0.0:5865 server.js",
    "debug": "NODE_OPTIONS='--experimental-global-webcrypto' nodemon --inspect=0.0.0.0:5865 --inspect-brk server.js",
    "reset-tenant": "node scripts/reset_tenant.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.15.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "dpop": "^1.0.0",
    "express": "^4.17.1",
    "express-openid-connect": "^2.8.0",
    "express-session": "^1.14.1",
    "jade": "^1.11.0",
    "jose": "^4.9.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "morgan": "^1.7.0",
    "nodemon": "^2.0.4",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.2",
    "passport-wsfed-saml2": "github:jhiner/passport-wsfed-saml2#orgs",
    "req-flash": "0.0.3"
  },
  "now": {
